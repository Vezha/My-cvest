int main()
{
	setlocale(LC_ALL, "rus");
	persone();
	Cvest();


	string path = "file1";
	ofstream fout;
	fout.open(path);
	if (!fout.is_open())
	{
		cout << "Ð¾ÑˆÐ¸Ð±ÐºÐ° Ð¾Ñ‚ÐºÑ€Ñ‹Ñ‚Ð¸Ñ Ñ„Ð°Ð¹Ð»Ð°!" << endl;
	}
	else
	{
		fout << "Ñ…Ð¾Ñ‚ÑŒ 8 Ð±ÑƒÐ´ÐµÑ‚?" << endl;
	}
	ifstream fin;
	fin.open(path);
	if (!fin.is_open())
	{
		cout << "Ð¾ÑˆÐ¸Ð±ÐºÐ° Ð¾Ñ‚ÐºÑ€Ñ‹Ñ‚Ð¸Ñ Ñ„Ð°Ð¹Ð»Ð°!" << endl;
	}
	else
	{
		cout << "Ñ„Ð°Ð¹Ð» Ð¾Ñ‚ÐºÑ€Ñ‹Ñ‚!" << endl;
		char ch;
		while (fin.get(ch))
		{
			cout << ch;
		}
	}
}
 
